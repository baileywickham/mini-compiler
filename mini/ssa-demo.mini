fun f(int x, int y) int
{
    int sum;
    sum = 3;
    sum = x + sum;
    x = y;
    if (x >= y)
    {
        return 0;
    }
    sum = 0;
    while (x < y)
    {
        if (x / 2 * 2 == x)
        {
            sum = sum + 1;
        }
        x = x + 1;
    }
    return sum;
}

